  type: bond
  state: up
  ipv4:
    enabled: false
  link-aggregation:
    mode: 802.3ad
    options:
      miimon: '100'
    port:
    - eth0
    - eth1
- name: bond0.4012
  type: vlan
  state: up
  vlan:
    base-iface: bond0
    id: 4012
  ipv4:
    address:
    - ip: 10.25.131.20
      prefix-length: 24
      gateway: 10.25.131.1
    enabled: true
